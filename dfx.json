{
  "canisters": {
    "candymachine": {
      "type": "custom",
      "build": "npx azle candymachine",
      "root": "canisters/candymachine",
      "ts": "canisters/candymachine/main.ts",
      "candid": "canisters/candymachine/main.did",
      "wasm": "target/wasm32-unknown-unknown/release/candymachine.wasm"
    },
    "candymachine_dip721": {
      "type": "custom",
      "build": "npx azle candymachine_dip721",
      "root": "canisters/candymachine_dip721",
      "ts": "canisters/candymachine_dip721/main.ts",
      "candid": "canisters/candymachine_dip721/main.did",
      "wasm": "target/wasm32-unknown-unknown/release/candymachine_dip721.wasm"
    },
    "candymachine_assets": {
      "dependencies": [
        "candymachine",
        "candymachine_dip721"
      ],
      "frontend": {
        "entrypoint": "canisters/candymachine_assets/src/index.html"
      },
      "source": [
        "canisters/candymachine_assets/assets",
        "dist/candymachine_assets/"
      ],
      "type": "assets"
    },
    "ledger": {
      "type": "custom",
      "wasm": "ledger.wasm",
      "candid": "ledger.public.did"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "dfx": "0.9.3",
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  },
  "version": 1
}